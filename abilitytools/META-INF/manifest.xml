<?xml version="1.0" encoding="UTF-8"?>
<manifest:manifest>
    <!--Add-on configuration: UI strings in different languages and path of icons-->
    <manifest:file-entry manifest:full-path="registry/data/org/openoffice/Office/Addons.xcu" manifest:media-type="application/vnd.sun.star.configuration-data"/>
    <manifest:file-entry manifest:full-path="registry/data/org/openoffice/Office/CalcWindowState.xcu" manifest:media-type="application/vnd.sun.star.configuration-data"/>
    <manifest:file-entry manifest:full-path="registry/data/org/openoffice/Office/ImpressWindowState.xcu" manifest:media-type="application/vnd.sun.star.configuration-data"/>
    <manifest:file-entry manifest:full-path="registry/data/org/openoffice/Office/WriterGlobalWindowState.xcu" manifest:media-type="application/vnd.sun.star.configuration-data"/>
    <manifest:file-entry manifest:full-path="registry/data/org/openoffice/Office/WriterWebWindowState.xcu" manifest:media-type="application/vnd.sun.star.configuration-data"/>
    <manifest:file-entry manifest:full-path="registry/data/org/openoffice/Office/WriterWindowState.xcu" manifest:media-type="application/vnd.sun.star.configuration-data"/>
    <!--Basic library: Path to Basic scripts-->
    <manifest:file-entry manifest:full-path="schooltools/" manifest:media-type="application/vnd.sun.star.basic-library"/>
    <!--Description: Show description when extension manager dialog is showing-->
    <manifest:file-entry manifest:media-type="application/vnd.sun.star.package-bundle-description" manifest:full-path="description/component.txt"/>
    <manifest:file-entry manifest:full-path="description/component.txt.en" manifest:media-type="application/vnd.sun.star.package-bundle-description;locale=en"/>
    <manifest:file-entry manifest:full-path="description/component.txt.fr" manifest:media-type="application/vnd.sun.star.package-bundle-description;locale=fr"/>
    <!--Help: Show help in the help menu-->
    <manifest:file-entry manifest:media-type="application/vnd.sun.star.help" manifest:full-path="help/"/>
</manifest:manifest>
