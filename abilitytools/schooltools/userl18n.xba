function fsWindowsShortFileName
	dim s1 as string
	on error goto fsWindowsShortFileNameError
	if NOT GlobalScope.BasicLibraries.isLibraryLoaded(&quot;Tools&quot;) then 
		GlobalScope.BasicLibraries.loadLibrary(&quot;Tools&quot;)
	endif
	s1=	GetFileNameWithoutExtension(ThisComponent.getURL(), &quot;/&quot;)
	if s1 = &quot;&quot; then
		fsWindowsShortFileName=fsGetSetting(&quot;ooname&quot;)&amp; &quot;%20&quot; &amp; fsGetSetting(&quot;ooversion&quot;) &amp; &quot;.odp&quot;
	else
		fsWindowsShortFileName = s1
	endif
	exit function
	fsWindowsShortFileNameError:
	fsWindowsShortFileName=fsGetSetting(&quot;ooname&quot;)&amp; &quot;%20&quot; &amp; fsGetSetting(&quot;ooversion&quot;) &amp; &quot;.odp&quot;
end function

