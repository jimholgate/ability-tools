	dim aSx
	dim i
	aSx = ThisComponent.DocumentProperties.Keywords
	fsKeywords = &quot;&quot;
	for i = LBound (aSx) to UBound (aSx)
		fsKeyWords = fsKeyWords &amp; &quot;, &quot; &amp; aSx(i)
	next
	fsKeyWords = trim(fsKeyWords)
