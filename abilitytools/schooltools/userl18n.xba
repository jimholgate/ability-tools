function fsTrimArticle (byval sA, optional sLanguage ) as string
	&apos; for titles in en and fr, move the initial article to the end
	&apos; for sorting purposes
	if ismissing(sLanguage) then 
		sLanguage= fsGetSetting(&quot;language&quot;)
	endif
	if len ( sA ) &lt;&gt; 0 then
		sA = lcase ( fsSuperTrim (sA) )
		select case sLanguage
		case &quot;fr&quot;
			&apos; Always use len function instead of guessing length 
			&apos; complex languages may not have the apparent length
			&apos; in all coding systems.
			if instr ( sA, &quot;la &quot;) = 1 then
				sA = mid(sA, len (&quot;la &quot;) + 1 ) &amp; &quot;, la&quot;
			elseif instr ( sA, &quot;le &quot;) = 1 then
				sA = mid(sA, len (&quot;le &quot;) + 1 ) &amp; &quot;, le&quot;
			elseif instr ( sA, &quot;les &quot;) = 1 then
				sA = mid(sA, len (&quot;les &quot;) + 1 ) &amp; &quot;, les&quot;
			elseif instr ( sA, &quot;un &quot;) = 1 then
				sA = mid(sA, len (&quot;un &quot;) + 1 ) &amp; &quot;, un&quot;
			elseif instr ( sA, &quot;une &quot;) = 1 then
				sA = mid(sA, len (&quot;une &quot;) + 1 ) &amp; &quot;, une&quot;
			elseif instr ( sA, &quot;l&apos;&quot;) = 1 then
				sA = mid(sA, len (&quot;l&apos;&quot;) + 1 ) &amp; &quot;, l&apos;&quot;
			elseif instr ( sA, &quot;l’&quot;) = 1 then
				sA = mid(sA, len (&quot;l’&quot;) + 1 ) &amp; &quot;, l’&quot;
			endif
		case &quot;en&quot;
			if instr ( sA, &quot;the &quot;) = 1 then
				sA = mid(sA, len (&quot;the &quot;) + 1) &amp; &quot;, the&quot;
			elseif instr ( sA, &quot;an &quot;) = 1 then
				sA = mid(sA, len (&quot;an &quot;) + 1) &amp; &quot;, an&quot;
			elseif instr ( sA, &quot;a &quot;) = 1 then
				sA = mid(sA, len (&quot;a &quot;) + 1) &amp; &quot;, a&quot;
			endif
		case else 
			fsTrimArticle = sA
		end select
	endif
		fsTrimArticle = sA
end function

function fsGetSetting(sA) as string
	if NOT GlobalScope.BasicLibraries.isLibraryLoaded(&quot;Tools&quot;) then 
		GlobalScope.BasicLibraries.loadLibrary(&quot;Tools&quot;)
	endif
	dim oProdNameAccess As object
	oProdNameAccess=GetRegistryKeyContent(&quot;org.openoffice.Setup/Product&quot;)
    select case lcase(sA)
    case &quot;language&quot;
		fsGetSetting=GetStarOfficeLocale().language
	case &quot;country&quot;
		fsGetSetting=GetStarOfficeLocale().country
	case &quot;ooname&quot;
		fsGetSetting=oProdNameAccess.getByName(&quot;ooName&quot;)
	case &quot;ooversion&quot;,&quot;oosetupversion&quot;
		fsGetSetting=oProdNameAccess.getByName(&quot;ooSetupVersion&quot;)
	case else 
		fsGetSetting=&quot;???&quot;
	end select
end function

